{
	"appName": "demo-GeneratorSpringboot",
	"packageName": "com.demo",
	"databaseType": "mysql",
	"table": [
		{
			"entityName": "Student",
			"fields": {
				"id": "Long",
				"Name": "String",
				"Age": "int",
				"Address": "String"
			}
		},
		{
			"entityName": "User",
			"fields": {
				"id": "Long",
				"username": "String",
				"password": "String"
			}
		}
	],
	"basePath": "/api/users",
	"dbMigrationTool": "flywaydb",
	"buildTool": "maven"
}
